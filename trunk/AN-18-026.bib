@Article{Frandsen2012,
author="Frandsen, Mads T. and Kahlhoefer, Felix and Preston, Anthony and Sarkar, Subir and Schmidt-Hoberg, Kai",
title="LHC and Tevatron bounds on the dark matter direct detection cross-section for vector mediators",
journal="Journal of High Energy Physics",
year="2012",
month="Jul",
day="20",
volume="2012",
number="7",
pages="123",
%abstract="We study the interactions of a new spin-1 mediator that connects the Standard Model to dark matter. We constrain its decay channels using monojet and monophoton searches, as well as searches for resonances in dijet, dilepton and diboson final states including those involving a possible Higgs. We then interpret the resulting limits as bounds on the cross-section for dark matter direct detection without the need to specify a particular model. For mediator masses between 300 and 1000 GeV these bounds are considerably stronger than the ones obtained under the assumption that the mediator can be integrated out.",
issn="1029-8479",
doi="10.1007/JHEP07(2012)123",
url="https://doi.org/10.1007/JHEP07(2012)123"
}

@Article{Alves2014,
author="Alves, Alexandre and Profumo, Stefano and Queiroz, Farinaldo S.",
title="The dark Z{\textasciiacutex} portal: direct, indirect and collider searches",
journal="Journal of High Energy Physics",
year="2014",
month="Apr",
day="09",
volume="2014",
number="4",
pages="63",
%abstract="We perform a detailed study of the dark Z{\textasciiacutex} portal using a generic parametrization of the Z{\textasciiacutex}-quarks couplings, both for light (8-15) GeV and heavy (100-1000) GeV dark matter scenarios. We present a comprehensive study of the collider phenomenology including jet clustering, hadronization, and detector artifacts, which allows us to derive accurate bounds from the search for new resonances in dijet events and from mono-jet events in the LHC 7 TeV, LHC 8 TeV, and Tevatron 1.96 TeV data. We also compute the dark matter relic abundance, the relevant scattering cross sections and pair-annihilation spectrum, and compare our results with the current PLANCK, Fermi-LAT and XENON100/LUX bounds. Lastly, we highlight the importance of complementary searches for dark matter, and outline the excluded versus still viable parameter space regions of the dark Z{\textasciiacutex} portal.",
issn="1029-8479",
doi="10.1007/JHEP04(2014)063",
url="https://doi.org/10.1007/JHEP04(2014)063"
}

@Article{Garny2014,
author="Garny, Mathias and Ibarra, Alejandro and Rydbeck, Sara and Vogl, Stefan",
title="Majorana dark matter with a coloured mediator: collider vs direct and indirect searches",
journal="Journal of High Energy Physics",
year="2014",
month="Jun",
day="27",
volume="2014",
number="6",
pages="169",
%abstract="We investigate the signatures at the Large Hadron Collider of a minimal model where the dark matter particle is a Majorana fermion that couples to the Standard Model via one or several coloured mediators. We emphasize the importance of the production channel of coloured scalars through the exchange of a dark matter particle in the t-channel, and perform a dedicated analysis of searches for jets and missing energy for this model. We find that the collider constraints are highly competitive compared to direct detection, and can even be considerably stronger over a wide range of parameters. We also discuss the complementarity with searches for spectral features at gamma-ray telescopes and comment on the possibility of several coloured mediators, which is further constrained by flavour observables.",
issn="1029-8479",
doi="10.1007/JHEP06(2014)169",
url="https://doi.org/10.1007/JHEP06(2014)169"
}

